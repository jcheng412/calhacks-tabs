{
  "name": "My Extension",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Gets information from Google.",
  "icons": {
    "128": "icon.jpg"
  },
  "background": {
    "persistent": false,
    "scripts": ["background_script.js"]
  },
  "permissions": ["https://*.google.com/", "activeTab"],
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  }
}
